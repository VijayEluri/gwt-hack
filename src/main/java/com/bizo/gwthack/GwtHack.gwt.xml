<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.4/distro-source/core/src/gwt-module.dtd">
<module rename-to="gwthack">

  <source path="client">
    <exclude name="**/Stub*" />
  </source>
  <source path="shared" />

  <inherits name="com.google.gwt.user.User" />             <!-- standard gwt components -->
  <inherits name="com.google.gwt.uibinder.UiBinder" />     <!-- uibinder -->
  <inherits name="com.google.gwt.resources.Resources" />   <!-- Client Bundles -->
  <inherits name="org.tessell.Tessell" />                  <!-- mvp model -->
  <inherits name="org.bindgen.Bindgen" />                  <!-- for Binding/AbstractBinding -->

  <entry-point class="com.bizo.gwthack.client.GwtHackApp" />

  <set-configuration-property name="CssResource.style" value="pretty"/>
</module>

<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:gwt="urn:import:com.google.gwt.user.client.ui"
  xmlns:mpv="urn:import:org.gwtmpv.widgets">
  <gwt:HTMLPanel tag="table">
    <tr>
      <td><gwt:Label ui:field="id"/></td>
      <td><gwt:Label ui:field="name"/></td>
      <td><gwt:Hyperlink ui:field="viewLink"/></td>
    </tr>
  </gwt:HTMLPanel>
</ui:UiBinder>